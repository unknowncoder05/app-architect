{
    "__constructor":{
        "__description": "a human model",
        "name":{"type":"str", "default":"name"},
        "birth":{"type":"date", "default":"birth"},
        "sector":{"type":"str", "default":"sector"}
    },
    "attributes":{
        "id": {"type":"id"},
        "__name":{"type":"str", "max":20, "required":true},
        "__birth":{"type":"date", "max":20, "required":true},
        "__sector":{"type":"str", "max":100, "required":true}
    },
    "create":{
        "request":{
            "__extends":{
                "__from":"attributes",
                "__excludes":["id"]
            }
        },
        "response":{
            "__extends":{
                "__from":"attributes",
                "__includes":["id"]
            }
        }
    },
    "list":{
        "response":{
            "__extends":{
                "__from":"attributes",
                "__includes":["__name"]
            }
        }
    },
    "get":{
        "request":{
                "__extends":{
                "__from":"attributes",
                "__includes":["id"]
            }
        },
        "response":{
            "__extends":{
                "__from":"attributes",
                "__excludes":["id"]
            }
        }
    }
}